<?xml version='1.0' encoding='utf-8'?>
<widget id="com.android.EaseUpCheckIn" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EaseUp Check-in</name>
    <description>
        EaseUp salon check-in application.
    </description>
    <author email="savienkov.ao@outlook.com" href="https://www.hys-enterprise.com/">
        EaseUp Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon id="icon" src="www/img/logo.png" />
    <icon height="255" id="logo" src="www/img/logo.png" width="255" />
</widget>
